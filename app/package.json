{
  "name": "wall-painter",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "Gerald <i@gerald.top>",
  "license": "ISC",
  "scripts": {
    "dev:web": "webpack-util develop",
    "dev:core": "cross-env ENTRY_PREFIX=http://localhost:3000 electron .",
    "dev:core:unix": "cross-env PYTHON=python/bin/python npm run dev:core",
    "dev:core:win": "cross-env PYTHON=python/scripts/python npm run dev:core",
    "prebuild": "npm run lint",
    "build": "webpack-util build",
    "lint": "eslint --ext .js,.vue .",
    "svgo": "svgo --config .svgo.yml src/resources/svg",
    "start": "electron .",
    "analyze": "cross-env RUN_ENV=analyze npm run build"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "main": "lib/index.js",
  "devDependencies": {
    "husky": "^1.3.1",
    "svgo": "^1.1.1",
    "webpack-util": "^0.1.23",
    "webpack-util-vue": "^0.1.17"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@gera2ld/rem": "^0.1.1",
    "axios": "^0.18.0",
    "core-js": "^2.6.1",
    "electron": "^4.0.0",
    "font-awesome": "^4.7.0",
    "spectre.css": "^0.5.8",
    "vue": "^2.5.21"
  }
}
